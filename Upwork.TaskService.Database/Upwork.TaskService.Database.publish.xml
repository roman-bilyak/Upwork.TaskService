<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>False</IncludeCompositeObjects>
    <TargetDatabaseName>TaskServiceDb</TargetDatabaseName>
    <DeployScriptFileName>../../Upwork.TaskService.Database.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=(LocalDb)\MSSQLLocalDB;Integrated Security=True;Persist Security Info=False;Pooling=False;MultipleActiveResultSets=False;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False</TargetConnectionString>
    <BlockOnPossibleDataLoss>False</BlockOnPossibleDataLoss>
    <CreateNewDatabase>True</CreateNewDatabase>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <PopulateFilesOnFileGroups>False</PopulateFilesOnFileGroups>
    <RestoreSequenceCurrentValue>False</RestoreSequenceCurrentValue>
    <DisableIndexesForDataPhase>False</DisableIndexesForDataPhase>
    <ScriptNewConstraintValidation>False</ScriptNewConstraintValidation>
    <UnmodifiableObjectWarnings>False</UnmodifiableObjectWarnings>
    <VerifyCollationCompatibility>False</VerifyCollationCompatibility>
    <VerifyDeployment>False</VerifyDeployment>
    <ScriptRefreshModule>False</ScriptRefreshModule>
    <DoNotAlterReplicatedObjects>False</DoNotAlterReplicatedObjects>
    <DoNotAlterChangeDataCaptureObjects>False</DoNotAlterChangeDataCaptureObjects>
    <DisableAndReenableDdlTriggers>False</DisableAndReenableDdlTriggers>
    <DropStatisticsNotInSource>False</DropStatisticsNotInSource>
    <DoNotEvaluateSqlCmdVariables>True</DoNotEvaluateSqlCmdVariables>
    <ScriptDatabaseOptions>False</ScriptDatabaseOptions>
    <CommentOutSetVarDeclarations>False</CommentOutSetVarDeclarations>
  </PropertyGroup>
</Project>